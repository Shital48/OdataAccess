<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<ViewSettingsDialog class="sapUiSizeCompact" confirm="handleConfirm" sortDescending="true" title="View Settings"
		sortItems="{AppModel>/sortingLookupData}" groupItems="{AppModel>/groupLookupData}">
		<sortItems>
			<ViewSettingsItem text="{AppModel>text}" key="{AppModel>key}"/>
		</sortItems>
		<groupItems>
			<ViewSettingsItem text="{AppModel>text}" key="{AppModel>key}"/>
		</groupItems>
	</ViewSettingsDialog>
</core:FragmentDefinition>